{
    "json": {
        "enabled": true,
        "output": "jsonlogs/log.json",
        "formater": "node_modules/@deepakvishwakarma/cucumber-json-formatter/lib/main.js"
    },
    "messages": {
        "enabled": true,
        "output": "jsonlogs/messages.ndjson"
    },
    "stepDefinitions": [
        "[filepath]/**/*.{js,ts}",
        "[filepath].{js,ts}",
        "cypress/e2e/step_definitions/*.{js,ts}"
    ]
}